            "target": "deb"
        }
    },
    "author": {
        "name": "Samuel Pedrera",
        "email": "samuelpedrera7@gmail.com"
    },
    "license": "MIT",
    "devDependencies": {
        "electron": "^30.1.0",
        "electron-builder": "^24.13.3"
    },
    "dependencies": {
        "emmet-monaco-es": "^5.3.2",
        "load-monaco": "^1.0.4",
        "monaco-editor": "^0.49.0",
        "trash": "^8.1.1"
    }
}
