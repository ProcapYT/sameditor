    "version": "2.0.0",
    "main": "src/main.js",
    "description": "A code editor very similar to vscode.",
    "scripts": {
        "start": "electron .",
        "build": "electron-builder",
        "build-win": "electron-builder --win",
        "build-linux": "electron-builder --linux"
